
public class Operacoes {
	 private static int valor;
	 private static int valor2;
	public static int getValor() {
		return valor;
	}
	public void setValor(int valor) {
		this.valor = valor;
	}
	public static int getValor2() {
		return valor2;
	}
	public void setValor2(int valor2) {
		this.valor2 = valor2;
	}
	 
	 
}	 
